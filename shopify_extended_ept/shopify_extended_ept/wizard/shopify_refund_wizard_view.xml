<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherite_view_shopify_refund_wizard" model="ir.ui.view">
            <field name="name">Inherit Refund In Shopify</field>
            <field name="model">shopify.cancel.refund.order.wizard</field>
            <field name="inherit_id" ref="shopify_ept.view_shopify_refund_wizard"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='restock_type']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='restock_type']" position="after">
                    <field name="restock_type"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>